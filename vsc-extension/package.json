{
  "name": "visual-editor-ai",
  "displayName": "Visual Editor - Visual-first AI coding agent",
  "description": "Visual-first AI coding agent for local codebases. Select elements in browser, describe changes, agent applies edits.",
  "version": "1.0.2",
  "publisher": "VisualEditor",
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages",
    "Machine Learning"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "visualEditorAI",
          "title": "Visual Editor AI",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "visualEditorAI": [
        {
          "id": "visualEditorAI.chromeExtension",
          "name": "Getting Started",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.19.19",
    "@types/vscode": "^1.104.0",
    "@vscode/vsce": "^2.32.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "ai",
    "coding",
    "agent",
    "radix ui",
    "styled components",
    "frontend",
    "visual editor",
    "react",
    "nextjs",
    "vue",
    "tailwind",
    "css",
    "html",
    "typescript",
    "javascript"
    
  ],
  "author": "Visual Editor AI Team",
  "license": "MIT",
  "type": "commonjs"
}
