{
  "name": "visual-editor-ai",
  "displayName": "Fronti AI",
  "description": "Visual-first AI coding agent for local codebases. Select elements in browser, describe changes, agent applies edits.",
  "version": "1.0.5",
  "publisher": "VisualEditor",
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages",
    "Machine Learning"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "FrontiAI",
          "title": "Fronti",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "FrontiAI": [
        {
          "id": "FrontiAI.chromeExtension",
          "name": "Getting Started",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "package": "vsce package"
  },
  "dependencies": {
    "@fronti/core": "file:../fronti-core/fronti-core-1.0.0.tgz"
  },
  "devDependencies": {
    "@types/node": "^20.19.19",
    "@types/vscode": "^1.104.0",
    "@vscode/vsce": "^2.32.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "ai",
    "coding",
    "agent",
    "radix ui",
    "styled components",
    "frontend",
    "react",
    "nextjs",
    "vue",
    "tailwind",
    "css",
    "html",
    "typescript",
    "javascript",
    "v0",
    "vibe",
    "dev",
    "bolt",
    "loveable",
    "base44",
    "dyad",
    "cursor",
    "windsurf",
    "replit",
    "stackblitz",
    "codesandbox",
    "ai code editor",
    "ai assistant",
    "code generation"
  ],
  "author": "Fronti",
  "license": "MIT",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/waspdev95/fronti"
  },
  "bugs": {
    "url": "https://github.com/waspdev95/fronti/issues"
  },
  "homepage": "https://github.com/waspdev95/fronti#readme"
}
